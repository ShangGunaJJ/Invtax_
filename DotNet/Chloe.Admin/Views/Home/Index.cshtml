<!DOCTYPE html>
<html style="overflow: hidden;">
<head>
    <title>Invtax发票管理系统</title>
    @Html.RefStyle("~/Content/bootstrap/bootstrap.min.css")
    @Html.RefStyle("~/Content/css/framework-font.css")
    @Html.RefStyle("~/Content/css/framework-theme.css")
    @Html.RefStyle("~/Content/css/index.css")

    @Html.RefScript("~/Content/jquery/jquery-1.11.1.min.js")
    @Html.RefScript("~/Content/bootstrap/bootstrap.min.js")
    @Html.RefScript("~/Content/dialog/dialog.js")
    @Html.RefScript("~/Content/jquery/jquery.cookie.js")
    @Html.RefScript("~/Content/js/framework-ui.js")
    @Html.RefScript("~/Content/js/framework-clientdata.js")
</head>
<body style="overflow: hidden;">
    <div id="ajax-loader" style="cursor: progress; position: fixed; top: -50%; left: -50%; width: 200%; height: 200%; background: #fff; z-index: 10000; overflow: hidden;">
        <img src="~/Content/img/ajax-loader.gif" style="position: absolute; top: 0; left: 0; right: 0; bottom: 0; margin: auto;" />
    </div>
    <div id="theme-wrapper">
        <header class="navbar" id="header-navbar">
            <div class="container" style="padding-right: 0px;">
                <a href="#" id="logo" class="navbar-brand">Invtax</a>
                <div class="clearfix">
                    <div class="nav-no-collapse navbar-left pull-left">
                        <ul class="nav navbar-nav pull-left">
                            <li>
                                <a class="btn" id="make-small-nav"><i class="fa fa-bars"></i></a>
                            </li>
                        </ul>
                    </div>
                 
                    <div class="nav-no-collapse pull-right" id="header-nav">
                        <ul class="nav navbar-nav pull-right">
                            <li class="dropdown profile-dropdown">
                                <a href="#" class="dropdown" data-toggle="dropdown">
                                    <img src="~/Content/img/samples/mrhua.png" alt="" />
                                    <span class="hidden-xs">@this.ViewBag.CurrentSession.RealName</span>
                                </a>
                                <ul class="dropdown-menu pull-right">
                                    <li><a class="menuItem" data-id="userInfo" href="@Url.Content("~/Account/Index")"><i class="fa fa-user"></i>个人信息</a></li>
                                    <li><a href="javascript:void();" onclick="layer.msg('浏览器缓存清除成功');"><i class="fa fa-trash-o"></i>清空缓存</a></li>
                                    <li><a href="javascript:void();"><i class="fa fa-paint-brush"></i>皮肤设置</a></li>
                                    <li class="divider"></li>
                                    <li><a href="@Url.Content("~/Account/Logout")"><i class="ace-icon fa fa-power-off"></i>安全退出</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <div class="nav-no-collapse navbar-left pull-right" style="line-height: 50px;color:white;font-size:14px;">
                        <i class="fa fa-bank"></i> @this.ViewBag.CompanyName
                    </div>
                </div>
            </div>
        </header>
        <div id="page-wrapper" class="container">
            <div class="row">
                <div id="nav-col">
                    <section id="col-left" class="col-left-nano">
                        <div id="col-left-inner" class="col-left-nano-content">
                            <div id="user-left-box" class="clearfix   dropdown profile2-dropdown">
                            </div>
                            <div class="navbar-collapse navbar-ex1-collapse" id="sidebar-nav">
                                <ul class="nav nav-pills nav-stacked">

                                </ul>
                            </div>
                        </div>
                    </section>
                </div>
                <div id="content-wrapper">
                    <div class="content-tabs">
                        <button class="roll-nav roll-left tabLeft">
                            <i class="fa fa-backward"></i>
                        </button>
                        <nav class="page-tabs menuTabs">
                            <div class="page-tabs-content" style="margin-left: 0px;">
                                <a href="javascript:;" class="menuTab active" data-id="Home">欢迎首页</a>
                            </div>
                        </nav>
                        <button class="roll-nav roll-right tabRight">
                            <i class="fa fa-forward" style="margin-left: 3px;"></i>
                        </button>
                        <div class="btn-group roll-nav roll-right">
                            <button class="dropdown tabClose" data-toggle="dropdown">
                                页签操作<i class="fa fa-caret-down" style="padding-left: 3px;"></i>
                            </button>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <li><a class="tabReload" href="javascript:void();">刷新当前</a></li>
                                <li><a class="tabCloseCurrent" href="javascript:void();">关闭当前</a></li>
                                <li><a class="tabCloseAll" href="javascript:void();">全部关闭</a></li>
                                <li><a class="tabCloseOther" href="javascript:void();">除此之外全部关闭</a></li>
                            </ul>
                        </div>
                        <button class="roll-nav roll-right fullscreen"><i class="fa fa-arrows-alt"></i></button>
                    </div>
                    <div class="content-iframe" style="background-color: #f9f9f9; overflow: hidden;">
                        <div class="mainContent" id="content-main" style="margin: 10px; margin-bottom: 0px; padding: 0;">
                            <iframe class="NFine_iframe" style="width:100%;height:100%;" src="Home/Home" frameborder="0" data-id="Home"></iframe>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="loadingPage" style="display: none;">
        <div class="loading-shade"></div>
    </div>

    @Html.RefScript("~/Content/js/index.js")
    @Html.RefScript("~/Content/js/indextab.js")
    @Html.RefScript("~/Content/loading/pace.min.js")
</body>
</html>
